SELECT
	SOURCE
	,COUNT(SOURCE) AS ACCT
	,MIN(LEN(NOLOAN)) AS MIN_
	,MAX(LEN(NOLOAN)) AS MAX_
FROM [dbo].[COLLSYS.REPORT.JOIN.DAILY]
WHERE
	(
		TGL_AKTIF_AKHIR IS NULL
		OR NOLOAN IS NULL
		OR NOMORCIF IS NULL
	)
AND LEFT(NOLOAN, 3) != 'RO '
AND LEFT(NOLOAN, 4) != 'AREA'
AND NOLOAN_FINAL IS NOT NULL
AND NOMORCIF IS NOT NULL
GROUP BY
	SOURCE;
